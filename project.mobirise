2\">\n\n        <input type=\"range\" title=\"Group4 Items\" inline name=\"group4Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >3\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup4\" value condition=\"groups>3\">\n\n        <input type=\"range\" title=\"Group5 Items\" inline name=\"group5Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >4\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup5\" value condition=\"groups>4\">\n\n        <input type=\"range\" title=\"Group6 Items\" inline name=\"group6Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >5\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup6\" value condition=\"groups>5\">\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" value=\"@PROJECT_PATH@/assets/images/background3.jpg\" title=\"Background Image\">\n            <input type=\"color\" value=\"#ffffff\" title=\"Background Color\" selected>\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=pjiVuLVqWQg\" title=\"Background Video\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        \n        <div class=\"py-3 ornament align-center\" mbr-if=\"showUnderline\">\n     