          "_name": "restaurant-menu2",
          "_customHTML": "<section class=\"restaurant-menu2 restaurant-special-menu\" group=\"Restaurant Menu\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"mbr-restaurant-special-menu\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"ShowUnderline\" name=\"showUnderline\">\n        <input type=\"color\" title=\"Underlines Color\" name=\"underlineColor\" value=\"#ffbc00\" condition=\"showUnderline\">\n\n        <input type=\"color\" title=\"Special Item\" name=\"specialColor\" value=\"#9f3942\">\n        <select title=\"Groups\" name=\"groups\">\n             <option value=\"2\">2</option>\n             <option value=\"3\">3</option>\n             <option value=\"4\" selected>4</option>\n             <option value=\"5\">5</option>\n             <option value=\"6\">6</option>\n        </select>\n        <input type=\"range\" title=\"Group1 Items\" inline name=\"group1Items\" min=\"1\" max=\"50\" value=\"11\" step=\"1\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup1\" value>\n\n        <input type=\"range\" title=\"Group2 Items\" inline name=\"group2Items\" min=\"1\" max=\"50\" value=\"6\" step=\"1\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup2\" value>\n\n        <input type=\"range\" title=\"Group3 Items\" inline name=\"group3Items\" min=\"1\" max=\"50\" value=\"7\" step=\"1\" condition=\"groups >2\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup3\" value condition=\"groups>2\">\n\n        <input type=\"range\" title=\"Group4 Items\" inline name=\"group4Items\" min=\"1\" max=\"50\" value=\"12\" step=\"1\" condition=\"groups >3\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup4\" value=\"7;8\" condition=\"groups>3\">\n\n        <input type=\"range\" title=\"Group5 Items\" inline name=\"group5Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >4\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup5\" value condition=\"groups>4\">\n\n        <input type=\"range\" title=\"Group6 Items\" inline name=\"group6Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >5\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup6\" value condition=\"groups>5\">\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" value=\"../_images/background5.jpg\" title=\"Background Image\">\n            <input type=\"color\" value=\"#ffffff\" title=\"Background Color\" selected>\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=pjiVuLVqWQg\" title=\"Background Video\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <h2 class=\"mbr-section-title align-center mbr-fonts-style mbr-black\" mbr-theme-style=